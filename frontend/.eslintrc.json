{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "import/no-named-as-default": 0,
        "camelcase": 0,
        "import/no-named-as-default-member": 0,
        "react/forbid-prop-types": 0,
        "import/prefer-default-export": 0,
        "quotes": 0,
        "comma-dangle": 0,
        "react/prop-types": 0

    }
}